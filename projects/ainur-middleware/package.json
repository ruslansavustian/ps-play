{
  "name": "ainur-middleware",
  "version": "1.0.0",
  "description": "Middleware for receipt processing between cash register and API",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "dev": "electron . --dev"
  },
  "keywords": [
    "electron",
    "middleware",
    "receipt",
    "cash-register"
  ],
  "author": "Ainur",
  "license": "ISC",
  "devDependencies": {
    "electron": "^29.1.4",
    "electron-builder": "^25.1.6"
  },
  "dependencies": {
    "express": "^4.21.2",
    "axios": "^1.7.9",
    "uuid": "^11.0.1"
  },
  "build": {
    "appId": "com.ainur.middleware",
    "productName": "Ainur Middleware",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "nsis",
      "icon": "icon.ico"
    },
    "files": [
      "main.js",
      "index.html",
      "renderer.js",
      "package.json",
      "node_modules/**/*"
    ]
  }
}
